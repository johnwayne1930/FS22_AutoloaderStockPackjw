                <wheels baseConfig="TRELLEBORG_DEFAULT">
                    <wheel filename="$data/shared/wheels/tires/bkt/ridemaxFL693M/500_45R22_5.xml">
                        <wheelChock/>
                        <wheelChock/>
                    </wheel>
                    <wheel filename="$data/shared/wheels/tires/bkt/ridemaxFL693M/500_45R22_5.xml"/>
                </wheels>
            </wheelConfiguration>
        </wheelConfigurations>

